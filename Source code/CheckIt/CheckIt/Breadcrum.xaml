<Window x:Class="CheckIt.Breadcrum"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CheckIt"
        mc:Ignorable="d"
        Title="Breadcrum" Height="170" Width="400">
    <Grid>
        <Button Name="Btn_ManageQuestions" Content="Manage Questions"
                FontFamily="Arial" FontSize="18"
                HorizontalAlignment="Left" 
                VerticalAlignment="Top" Width="400" 
        Height="50" Margin="0,0,0,0" 
                Click="Btn_ManageQuestions_Click"/>
        <Button Name="Btn_Settings" Content="Settings"
                FontFamily="Arial" FontSize="18"
                HorizontalAlignment="Left" 
                VerticalAlignment="Top" Width="400" 
        Height="50" Margin="0,48,0,0" 
                Click="Btn_Settings_Click"/>
        <Button Name="Btn_ViewResult" Content="View Result"
                FontFamily="Arial" FontSize="18"
                HorizontalAlignment="Left" 
                VerticalAlignment="Top" Width="400" 
        Height="50" Margin="0,98,0,0" 
                Click="Btn_ViewResult_Click"/>
        
    </Grid>
</Window>
